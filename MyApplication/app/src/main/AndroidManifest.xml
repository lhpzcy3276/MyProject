<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kangxin.myproject">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.MyShowLightDialog"
            android:theme="@style/activity_DialogTransparent" />
        <activity android:name=".activity.MoneyInputActivity" />
        <activity android:name=".activity.OkhttpUtilsActivity" />
        <activity android:name=".activity.RetrofitActivity" />
        <activity android:name=".activity.RxJavaActivity" />
        <activity android:name=".activity.ButterKnifeActivity"></activity>
    </application>

</manifest>